{
  "name": "@softonix/eslint-config-vue",
  "version": "0.1.2",
  "description": "Softonix ESLint configuration",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/softonix/softonix-packages/tree/main/packages/eslint-config-vue#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Softonix/softonix-packages.git",
    "directory": "packages/eslint-config-vue"
  },
  "author": "Softonix LLC",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint --fix --cache --max-warnings 0 .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "catalog:dev"
  },
  "dependencies": {
    "@softonix/eslint-config-rules-base": "workspace:*",
    "@softonix/eslint-config-rules-stylistic": "workspace:*",
    "@softonix/eslint-config-rules-typescript": "workspace:*",
    "@stylistic/eslint-plugin": "catalog:dev",
    "@vue/eslint-config-typescript": "catalog:dev",
    "eslint-flat-config-utils": "catalog:dev",
    "eslint-plugin-vue": "catalog:dev",
    "typescript-eslint": "catalog:dev"
  }
}
