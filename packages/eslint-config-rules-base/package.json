{
  "name": "@softonix/eslint-config-rules-base",
  "version": "0.1.0",
  "description": "Base ESLint rules for Softonix packages",
  "type": "module",
  "license": "MIT",
  "author": "Softonix LLC",
  "homepage": "https://github.com/softonix/softonix-packages/tree/main/packages/eslint-config-rules-base#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/softonix/softonix-packages.git",
    "directory": "packages/eslint-config-rules-base"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint --fix --cache --max-warnings 0 .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "catalog:dev"
  }
}